{
  "name": "kobeni",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently -c \"blue,yellow\" -n \"Client,Server\" \"npm run client:dev\" \"npm run server:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/app/**/*\" --exec \"npm run build && node dist/server.cjs\"",
    "server:build": "esbuild ./src/server.ts --bundle --format=cjs --outfile=dist/server.cjs --platform=node",
    "assets:init": "esbuild ./src/init_files.ts --bundle --format=cjs --outfile=dist/init_files.cjs --platform=node && node dist/init_files.cjs",
    "client:build": "vite build",
    "build": "npm run server:build && npm run client:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "prettier --check . && eslint . && stylelint \"**/*.css\"",
    "start": "node dist/server.cjs",
    "lint": "prettier -w .",
    "compile": "npm run build && npm run assets:init && nexe dist/server.cjs -o dist/server.exe -r dist/app/**/* -r dist/assets/**/* --build -t windows --verbose --name kobeni"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.1",
    "@tabler/icons": "1.115.0",
    "@tanstack/react-query": "4.19.1",
    "@tanstack/react-router": "0.0.1-beta.28",
    "axios": "1.2.0",
    "cli-progress": "3.11.2",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "framer-motion": "7.6.19",
    "got": "12.5.3",
    "ip": "1.1.8",
    "needle": "3.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "redux": "4.2.0",
    "socket.io": "4.5.4",
    "socket.io-client": "4.5.4",
    "ts-retry-promise": "0.7.0",
    "ws": "8.11.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@emotion/react": "11.10.5",
    "@mantine/core": "5.9.1",
    "@mantine/dropzone": "5.9.1",
    "@mantine/form": "5.9.1",
    "@mantine/hooks": "5.9.1",
    "@mantine/notifications": "5.9.1",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.21",
    "@types/cli-progress": "3.11.0",
    "@types/express": "4.17.14",
    "@types/ip": "1.1.0",
    "@types/needle": "2.5.3",
    "@types/node": "18.11.10",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@vitejs/plugin-react": "2.1.0",
    "concurrently": "7.6.0",
    "esbuild": "0.15.18",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "nexe": "4.0.0-rc.1",
    "nodemon": "2.0.20",
    "prettier": "2.8.0",
    "rimraf": "3.0.2",
    "stylelint": "14.16.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "vite": "3.1.0",
    "vite-plugin-eslint": "1.8.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
